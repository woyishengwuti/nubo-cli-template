{
  "name": "nb_cloud_platform",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev:m": "vue-cli-service serve --mode modules",
    "build:m": "cross-env VUE_APP_FRAME=dashboard vue-cli-service build",
    "dev:tran": "vue-cli-service serve --mode tran",
    "build:tran": "cross-env VUE_APP_FRAME=transboard vue-cli-service build",
    "lint": "eslint --fix --ext .js,.vue src",
    "dll": "webpack -p --progress --config ./webpack.dll.conf.js",
    "test:unit": "vue-cli-service test:unit",
    "test:cov": "vue-cli-service test:unit --coverage",
    "prepare": "husky install",
    "lint:eslint": "eslint --fix --ext \"src/**/*.{vue,less,css,scss}\"",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^3.6.2",
    "echarts": "^4.8.0",
    "element-ui": "^2.13.0",
    "file-saver": "^2.0.0",
    "js-cookie": "^2.2.1",
    "loadjs": "^4.2.0",
    "moment": "^2.24.0",
    "vant": "^2.10.2",
    "vue": "^2.6.11",
    "vue-i18n": "^8.15.3",
    "vue-lazyload": "^1.3.3",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.7.7",
    "@babel/register": "^7.7.7",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@vue/cli-plugin-babel": "^4.1.2",
    "@vue/cli-plugin-eslint": "^4.1.2",
    "@vue/cli-service": "^4.3.1",
    "@vue/test-utils": "^1.0.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "8.0.1",
    "babel-polyfill": "^6.26.0",
    "compression-webpack-plugin": "^3.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "6.1.2",
    "husky": "^7.0.2",
    "inquirer": "^7.0.1",
    "lint-staged": "^11.1.2",
    "mockjs": "1.1.0",
    "node-sass": "^4.14.0",
    "postcss-plugin-px2rem": "^0.8.1",
    "prettier": "^2.3.2",
    "sass-loader": "^8.0.0",
    "sass-resources-loader": "^2.0.1",
    "svg-sprite-loader": "^4.2.5",
    "vue-template-compiler": "^2.6.11",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.10"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
