{
  "name": "nb_cloud_platform",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev:phar": "vue-cli-service serve --mode phar",
    "build:phar": "cross-env VUE_APP_FRAME=dashboard vue-cli-service build",
    "dev:tran": "vue-cli-service serve --mode tran",
    "build:tran": "cross-env VUE_APP_FRAME=transboard vue-cli-service build",
    "lint": "eslint --fix --ext .js,.vue src",
    "dll": "webpack -p --progress --config ./webpack.dll.conf.js",
    "test:unit": "vue-cli-service test:unit",
    "test:cov": "vue-cli-service test:unit --coverage",
    "build:phar:b": "cross-env BundleAnalyzerPlugin=true npm run build:m",
    "build:tran:b": "cross-env BundleAnalyzerPlugin=true npm run build:m"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^3.6.2",
    "echarts": "^4.8.0",
    "element-ui": "^2.13.0",
    "file-saver": "^2.0.0",
    "js-cookie": "^2.2.1",
    "loadjs": "^4.2.0",
    "moment": "^2.24.0",
    "vant": "^2.10.2",
    "vue": "^2.6.11",
    "vue-i18n": "^8.15.3",
    "vue-lazyload": "^1.3.3",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.7.7",
    "@babel/register": "^7.7.7",
    "@vue/cli-plugin-babel": "^4.1.2",
    "@vue/cli-plugin-eslint": "^4.1.2",
    "@vue/cli-service": "^4.3.1",
    "@vue/test-utils": "^1.0.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "8.0.1",
    "babel-polyfill": "^6.26.0",
    "compression-webpack-plugin": "^3.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "6.1.2",
    "inquirer": "^7.0.1",
    "mockjs": "1.1.0",
    "node-sass": "^4.14.0",
    "postcss-plugin-px2rem": "^0.8.1",
    "sass-loader": "^8.0.0",
    "sass-resources-loader": "^2.0.1",
    "svg-sprite-loader": "^4.2.5",
    "vue-template-compiler": "^2.6.11",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.10"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
